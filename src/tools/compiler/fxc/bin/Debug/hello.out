include "a32.inc"
; Flouronix Compiler Standard Library routines.
_print:
PUSH R0
PUSH R1
.loop:
LOAD_BYTE
CMPR R1, 0
JMPF_E .quit
VM_CALL 0x0
JMPF .loop
.quit:
POP R1
POP R0
RETF
_read:
PUSH R0
PUSH R1
.loop:
VM_CALL 0x1
CMPR R1, 13
JMPF_E .quit
CMPR R1, 0
JMPF_E .loop
VM_CALL 0x0
STORE_BYTE
JMPF .loop
.quit:
POP R1
POP R0
RETF
_strcmp:
MEMCMP
RETF
_memcpy:
MEMCPY
RETF
_start:
_main:
LOAD_R0 __0
CALLF _print
LOAD_R0 i
CALLF _read
PUSH 2
PUSH 2
LOAD_FROM_SP  2
LOAD_R0 R1
LOAD_FROM_SP  1
CMPR R1, R0
JMPF_E _lbl_0
JMPF @f
_lbl_0:
LOAD_R0 __1
CALLF _print
@@:
VM_EXIT
_end_start:
_data:
__0: db 'hello world!', 0
__1: db 'hello again', 0
i: times 256 db 0
_end_data:
_bss:
_end_bss:
